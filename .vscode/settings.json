{
  // "explorer.sortOrder": "mixed",
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.autoImportFileExcludePatterns": ["@base-ui/*", "cmdk"],
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "editor.defaultFormatter": "biomejs.biome",
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(((?:[^()]|\\([^()]*\\))*)\\)", "[\"'`]?([^\"'`]+)[\"'`]?"],
    ["cn\\(((?:[^()]|\\([^()]*\\))*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "tailwindCSS.classAttributes": ["class", "className", "classNames", ".*Styles*", ".*ClassName*", ".*ClassNames*"],
  "workbench.editor.customLabels.patterns": {
    "**/**/layout.tsx": "${dirname} - layout"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "//": "Last update at 11/01/2025, 16:44:32",
    ".env": "*.env, .env.*, .envrc, env.d.ts",
    "next.config.*": ".next*, *.env, env.d.ts, next-env.d.ts, playwright.config.*, postcss.config.*, puppeteer.config.*, tailwind.config.*, tsconfig.*, reset.d.ts, sentry*, components.json",
    "package.json": ".cursorignore, .gitignore, reset.d.ts, .cursorrules, .github*, turbo.json, .vscode*, biome.json*, bun.lock, bun.lockb, knip.*, pnpm*, renovate*, vercel*, .npmrc",
    "app.config.*": "eas.json, expo-env.d.ts, validate-imports.js, metro.config.*, google-services*.json, nativewind-env.d.ts, tailwind.config.*, tsconfig.*, globals.css, babel.config.*"
  }
}
